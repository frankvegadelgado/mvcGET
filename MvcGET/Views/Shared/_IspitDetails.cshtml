@model IEnumerable<MvcGET.Models.Ispit>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Predmet.Tema)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ocena)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Predmet.Tema)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ocena)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", "Ispits", new { id = item.BI }, null) |
                @Html.ActionLink("Details", "Details", "Ispits", new { id = item.BI }, null) |
                @Html.ActionLink("Delete", "Delete", "Ispits", new { id = item.BI }, null)
            </td>
        </tr>
    }

</table>

